#[T=application/x-httpd-cgi]




#RewriteEngine   On
#RewriteMap      cgiauth                                 prg:/srv/max/nfshome/project/www/wiki/CGIAuth.pl

#RewriteCond     %{HTTP:Authorization}                   ^Basic                                  [NC]
