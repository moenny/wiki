##RewriteRule  ^([a-t].*) /project.new/wiki/wiki.cgi/$1  [L,PT]
#RewriteCond %{REQUEST_URI} !^/project.new/wiki/(.*)\.(cgi|js|css)
#RewriteRule  ^(.*) /project.new/wiki/wiki.cgi/$1  [L,PT]


#DirectoryIndex wiki.cgi

#RewriteRule    wiki\.(cgi|css)(.*)$  - [L]

#RewriteRule    [^\/]+/wiki.css     wiki.css  [L]
